<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
        
<hibernate-mapping>
	
    <class name="hiber.User" table="User">
   
        <id name="uid" type="java.lang.Integer" column="uid">			
			<generator class="hilo" />
		</id>
        <property name="uname" type="string" column="uname"/>
        <property name="upass" type="string" column="upass"/>
        <property name="flag" type="integer" column="flag"/>
     
	</class>
	
</hibernate-mapping>
